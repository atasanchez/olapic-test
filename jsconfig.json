{
  "compilerOptions": {
    "target": "es2017",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "@config/*": [
        "src/config/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@services/*": [
        "src/servicies/*"
      ],
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}