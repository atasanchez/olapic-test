{
  "extends": "airbnb",
  "settings": {
    "import/resolver": "webpack"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "linebreak-style": [
      "error",
      "windows"
    ],
    "react/prop-types": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "class-methods-use-this": 0,
    "import/no-unresolved": 0,
    "no-plusplus": 0,
    "no-param-reassign": 0,
    "no-var": 0,
    "no-new": 0,
    "react/destructuring-assignment": 0
  }
}